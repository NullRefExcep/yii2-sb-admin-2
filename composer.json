{
    "name": "nullref/yii2-sb-admin-2",
    "description": "Collection of asset bundles and other stuff for SB Admin theme",
    "type": "yii2-extension",
    "keywords": ["yii2","extension"],
    "license": "MIT",
    "authors": [
        {
            "name": "Dmytro Karpovych",
            "email": "zayec77@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "yiisoft/yii2": ">=2.0.4",
        "yiisoft/yii2-bootstrap": "*",
        "rmrevin/yii2-fontawesome": ">=2.10.0",
        "bower-asset/startbootstrap-sb-admin-2": "dev-master"
    },
    "extra": {
        "bower-asset-ignore": [
            "raphael"
        ],
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    },
    "autoload": {
        "psr-4": {
            "nullref\\sbadmin\\": "src/"
        }
    }
}
