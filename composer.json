{
    "name": "nullref/yii2-sb-admin-2",
    "description": "Collection of asset bundles and other stuff for SB Admin theme",
    "type": "yii2-extension",
    "keywords": ["yii2","extension"],
    "license": "MIT",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Dmytro Karpovych",
            "email": "zayec77@gmail.com"
        }
    ],
    "require": {
        "yiisoft/yii2-bootstrap": "~2.0.0",
        "rmrevin/yii2-fontawesome": "^2.16",
        "bower-asset/metismenu": "1.1.3"
    },
    "autoload": {
        "psr-4": {
            "nullref\\sbadmin\\": "src/"
        }
    },
    "config": {
        "fxp-asset": {
            "vcs-driver-options": {
                "github-no-api": true
            },
            "pattern-skip-version": "(-build|-patch)"
        }
    }
}
